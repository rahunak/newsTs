{
    "plugins": ["prettier", "import","@typescript-eslint"],
    "extends": [
        "plugin:prettier/recommended",
        "prettier"
    ],
    "parserOptions": {
        "ecmaVersion": 2020,
        "sourceType": "module"
    },
    "env": {
        "es6": true,
        "browser": true,
        "node": true
    },
    "rules": {
        "@typescript-eslint/rule-name": "error",
        "no-debugger": "off",
        "no-console": 0,
        "class-methods-use-this": "off"
    },
    "parser": "@typescript-eslint/parser",
    "prettier/prettier": [
        "error",
        {
          "endOfLine": "auto"
        }
      ]
   
 
}
